<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:tab.general</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.pages>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:pages</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>3</size>
                                <maxitems>10</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.pages>  
                    <settings.slideItems>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:slideItems</label>
                            <config>
                                <form_type>user</form_type>
                                <userFunc>RZ\Owlcarousel\Hooks\ItemsProcFunc->user_slideItems</userFunc>
                            </config>
                        </TCEforms>
                    </settings.slideItems>                   
                </el>
            </ROOT>
        </sDEF>
        <responsive>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:tab.responsive</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.items>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:items</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <eval>trim</eval>
                                <default>3</default>
                            </config>
                        </TCEforms>
                    </settings.items>
                    <settings.responsive>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:responsive</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                            <onChange>reload</onChange>
                        </TCEforms>
                    </settings.responsive>
                    <settings.items992>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:items992</label>
                            <displayCond>FIELD:settings.responsive:REQ:true</displayCond>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <eval>trim</eval>
                                <default>2</default>
                            </config>
                        </TCEforms>
                    </settings.items992>
                    <settings.items768>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:items768</label>
                            <displayCond>FIELD:settings.responsive:REQ:true</displayCond>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <eval>trim</eval>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.items768>
                    <settings.items0>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:items0</label>
                            <displayCond>FIELD:settings.responsive:REQ:true</displayCond>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <eval>trim</eval>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.items0>
                </el>
            </ROOT>
        </responsive>
        <options>
