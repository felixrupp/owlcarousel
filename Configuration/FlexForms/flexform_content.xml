                <el>
                    <settings.slideBy>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:slideBy</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <eval>trim</eval>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.slideBy> 
                    <settings.navSpeed>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:navSpeed</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <eval>trim,int</eval>
                                <default>1000</default>
                            </config>
                        </TCEforms>
                    </settings.navSpeed> 
                    <settings.dotsSpeed>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:dotsSpeed</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <eval>trim,int</eval>
                                <default>1000</default>
                            </config>
                        </TCEforms>
                    </settings.dotsSpeed>  
                    <settings.startPosition>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:startPosition</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <eval>trim,int</eval>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.startPosition>   
                    <settings.autoplay>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:autoplay</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.autoplay>  
                    <settings.autoplayHoverPause>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:autoplayHoverPause</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.autoplayHoverPause>  
                    <settings.autoplayTimeout>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:autoplayTimeout</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <eval>trim,int</eval>
                                <default>5000</default>
                            </config>
                        </TCEforms>
                    </settings.autoplayTimeout>  
                    <settings.nav>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:nav</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.nav>  
                    <settings.dots>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:dots</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.dots>  
                    <settings.loop>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:loop</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.loop> 
                    <settings.autoWidth>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:autoWidth</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.autoWidth> 
                    <settings.margin>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:margin</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <eval>trim,int</eval>
                                <default>15</default>
                            </config>
                        </TCEforms>
                    </settings.margin>
                    <settings.dotsEach>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:dotsEach</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <eval>trim,int</eval>
                            </config>
                        </TCEforms>
                    </settings.dotsEach>
                    <settings.mouseDrag>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:mouseDrag</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.mouseDrag> 
                    <settings.touchDrag>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:touchDrag</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
