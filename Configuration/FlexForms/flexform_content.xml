            </ROOT>
        </options>
        <template>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:tab.template</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.templateLayout>
                        <TCEforms>
                        <exclude>1</exclude>
                        <label>LLL:EXT:owlcarousel/Resources/Private/Language/locallang_flex.xlf:templateLayout</label>
                            <config>
                                <type>select</type>
                                <itemsProcFunc>RZ\Owlcarousel\Hooks\ItemsProcFunc->user_templateLayout</itemsProcFunc>
                                <items>
                                    <numIndex index="0" type="array">
                                        <numIndex index="0"></numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.templateLayout>
                </el>
            </ROOT>
        </template>
    </sheets>
</T3DataStructure>